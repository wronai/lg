{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Total Function Calls",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(nfo_calls_total)",
          "legendFormat": "Total Calls",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 1000 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Total Errors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "sum(nfo_errors_total)",
          "legendFormat": "Total Errors",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(nfo_errors_total[5m])) / sum(rate(nfo_calls_total[5m])) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Avg Duration (ms)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(nfo_duration_seconds_sum[5m])) / sum(rate(nfo_duration_seconds_count[5m])) * 1000",
          "legendFormat": "Avg ms",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Calls per Second (by function)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "rate(nfo_calls_total[1m])",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops" },
        "overrides": []
      }
    },
    {
      "title": "Errors per Second (by function)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "rate(nfo_errors_total[1m])",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "title": "Duration Distribution (histogram)",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "rate(nfo_duration_seconds_bucket[5m])",
          "legendFormat": "{{le}}s",
          "refId": "A",
          "format": "heatmap"
        }
      ]
    },
    {
      "title": "p95 Duration by Function",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(nfo_duration_seconds_bucket[5m]))",
          "legendFormat": "p95 {{function}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Calls by Level",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum by (level) (nfo_calls_total)",
          "legendFormat": "{{level}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Top Functions by Call Count",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 20 },
      "targets": [
        {
          "expr": "topk(10, sum by (function) (nfo_calls_total))",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Top Functions by Error Count",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 20 },
      "targets": [
        {
          "expr": "topk(10, sum by (function) (nfo_errors_total))",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" }
        },
        "overrides": []
      }
    }
  ],
  "refresh": "5s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["nfo", "logging", "monitoring"],
  "templating": { "list": [] },
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
